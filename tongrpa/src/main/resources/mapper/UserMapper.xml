<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.skcc.tongrpa.user.UserDao" >

   <select id="getUserList" resultType="com.skcc.tongrpa.user.UserModel">
       select * from USER_INFO
       
   </select>    
   <select id="getUserInfo" resultType="com.skcc.tongrpa.user.UserModel">
       select * from USER_INFO WHERE USER_ID=#{userId}
   </select>    
      
  <insert id="insertUser">
    INSERT INTO USER_INFO(USER_ID, USER_NM, CHBOT_KEY, USER_TYP,REG_USER)
    VALUES(#{userId}, #{userNm}, #{chbotKey}, #{userTyp} , #{regUser})
  </insert>
 
  <update id="updateUser">
    UPDATE USER_INFO 
    SET
     USER_NM= #{userNm},
     CHBOT_KEY = #{chbotKey},
     USER_TYP = #{userTyp}
    WHERE USER_ID = #{userId}
  </update>
 
  <delete id="deleteUser">
    DELETE FROM USER_INFO WHERE USER_ID = #{userId}
  </delete>
  
</mapper>